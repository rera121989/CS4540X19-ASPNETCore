@model List<Employee>

<h1>Employees</h1>

<table class="table">
  <thead>
    <tr>
      <th>Id</th>
      <th>Name</th>
      <th>Date Hired</th>
      <th>Supervisor</th>
    </tr>
  </thead>
  <tbody>
    @foreach (var employee in Model)
    {
      <tr>
        <td>@employee.EmployeeId</td>
        <td>@employee.Name</td>
        <td>@employee.DateHired</td>
        <td>@employee.Supervisor?.Name</td>
      </tr>
    }
  </tbody>
</table>
