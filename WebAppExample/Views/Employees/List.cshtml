@model List<Employee>

<h1>Employees</h1>

<table class="table">
  <thead>
    <tr>
      <th>Id</th>
      <th>Name</th>
      <th>Date Hired</th>
      <th>Supervisor</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    @foreach (var employee in Model)
    {
      <tr>
        <td>@employee.EmployeeId</td>
        <td><a href="View/@employee.EmployeeId">@employee.Name</a></td>
        <td>@employee.DateHired</td>
        <td>@employee.Supervisor?.Name</td>
        <td><a href="Edit/@employee.EmployeeId">Edit</a></td>
      </tr>
    }
  </tbody>
</table>

